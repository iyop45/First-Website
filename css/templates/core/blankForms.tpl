{strip}
.{#prefixBlankForms#}inner{
	width: 940px;
	margin: 0 auto;
	padding-top: 50px;
}
.{#prefixBlankForms#}form_box{
	margin-bottom: 0;
	width: 640px;
	-webkit-border-radius: 4px;
	margin-left: auto;
	margin-right: auto;
	background-color: #fff;
	padding-top: 10px;
}
.{#prefixBlankForms#}header{
	line-height: 20px;
	min-height: 20px;
	padding-bottom: 2px;
	border-bottom: 1px solid #aaa;
	margin-left: 20px;
	margin-right: 20px;
	color: #1c2a47;
	font-size: 16px;
	font-weight: 700;
}
.{#prefixBlankForms#}form_inner{
	padding-left: 20px;
	padding-right: 20px;
	padding-top: 10px;
	margin-bottom: 15px;
}
.{#prefixBlankForms#}form_inner h3{
	margin: 0 10px 10px;
}
.{#prefixBlankForms#}form_wrap{
	position: relative;
	width: 100%;
}
.{#prefixBlankForms#}form_wrap form{
	width: 100%;
}
.{#prefixBlankForms#}form_wrap form .label{
	margin: 0 0 5px;
}
.{#prefixBlankForms#}error_list{
	color: rgb(170, 34, 34);
	font-size: 13px;
	width: 300px;
	margin: 0 0 5px 25%;
	position: relative;
	left: 15px;
}
{/strip}