{strip}
.{#prefixAbout#}logo{
	margin: 0 auto 20px auto;
	display:block;
}
.{#prefixAbout#}title{
	font-size: 25px!important;
	color: #363636;
	font-weight: bold;
	text-shadow: 0 1px 0 #fff;
	text-align: center;
	margin-bottom:40px;
}
.{#prefixAbout#}content_block{
	font-family: Trebuchet MS,Liberation Sans,DejaVu Sans,sans-serif;
	width: 900px;
	margin: 20px auto 10px auto;
	padding-top: 30px;
	padding-bottom: 20px;
	font-size: 16px;
	overflow:hidden;
	line-height: 1.4;
	position: relative;
}
.{#prefixAbout#}button_wrapper{
	width:175px;
	margin:0 auto;
}
.{#prefixAbout#}hr{
	border-top: none;
	border-bottom: 1px solid rgba(0,0,0,0.1);
}
.{#prefixAbout#}stats_wrapper{
	margin: 30px 0 0;
	float: left;
}
.{#prefixAbout#}stats{
	list-style-type: none;
	margin: 0 0 40px 0;
	padding-left: 0;
	padding-bottom: 25px;
	border-bottom: 1px solid #d1d3d6;
	border-bottom: 1px solid rgba(0,0,0,0.1);
	overflow:hidden;
}
.{#prefixAbout#}stats li{
	float:left;
	padding-right:65px;
	padding-left:30px;
	border-right: 1px solid #efefef;
	border-right: 1px solid rgba(0,0,0,0.067);
}
.{#prefixAbout#}number{
	font-weight: bold;
	font-size: 50px;
	color: #888;
	margin: 0;
	height: 45px;
	line-height: 100%;
}
.{#prefixAbout#}description{
	color: #363636;
}
.{#prefixAbout#}last{
	border-right: 0!important;
	padding-right: 0!important;
}
.{#prefixAbout#}h3{
	color: #363636;
	font-size: 18px;
	font-weight: bold;
	margin-bottom:7px;
}
p.{#prefixAbout#}large{
	font-size: 18px;
	font-family: "Helvetica Neue","HelveticaNeue",Helvetica,Arial,sans-serif;
	color: #444;
	margin-bottom:15px;
}
.{#prefixAbout#}brief{
	float: left;
	text-align: left;
	width: 500px;
}
.{#prefixAbout#}info{
	padding:20px;
	position: relative;
	margin-left:20px;
	list-style: none;
	border-radius: 3px;
	width:300px;
	float:left;
	font-size: 14px;
	color: #444;
	background: #f4f4f4
}
.{#prefixAbout#}info_item{
	overflow: hidden;
	clear: both;
	line-height: 30px
}
.{#prefixAbout#}label{
	float: left;
	background-color: #f4f4f4;
	padding-right: 10px;
}
.{#prefixAbout#}value{
	float: right;
	background-color: #f4f4f4;
	padding-left: 10px;
}
{/strip}