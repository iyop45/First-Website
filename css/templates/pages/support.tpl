{strip}
#{#prefixSupport#}b{
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	-ms-border-radius: 4px;
	-o-border-radius: 4px;
	border-radius: 4px;
	-webkit-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	-moz-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	-ms-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	-o-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	background: #fff;
	float: left;
	margin: 50px 10px 60px 10px;
	padding-bottom: 40px;
	font-family: 'WOL_SL','Segoe UI Semilight','Segoe UI',Tahoma,Helvetica,sans-serif;
	font-size: 14px;
	padding-left: 20px;
	max-width: 580px;
}
.{#prefixSupport#}h{
	line-height: 50px;
	font-weight: 200;
	font-size: 25px;
	line-height: 50px;
	-webkit-font-feature-settings: "kern" 1;
	-moz-font-feature-settings: "kern" 1;
	font-feature-settings: "kern" 1;
	margin-bottom: 15px;
}
#{#prefixSupport#}b p{
	color: #636468;
	font-family: 'Segoe UI', Arial, Sans-Serif;
	font-size: 14px;
	margin: 10px 0;
	padding-right: 20px;
}
#{#prefixSupport#}b h2, #float_feedback h2{
	padding-top: 10px;
	color: #336699;
	font-size: 16px;
	font-weight: bold;
	margin-right: 20px;
}
#{#prefixSupport#}b h2{
	border-top: 1px solid #e2e3e2;
}
/** Center our page in the middle of the window **/
div#{#prefixSupport#}wrapper{
	width:960px;
	margin: 0 auto;
	position:relative; /** Important later on **/
}
/** Give our primary column a width and float it to the left width a right margin **/
div#{#prefixSupport#}primary{
	width:720px;
	margin: 20px 40px 0 0;
	float:left;
	display:inline;
	font: 14px/24px helvetica, arial, sans-serif;
}
/** Position our sidebar to the right of the primary content bar **/
#{#prefixSupport#}sidebar {
	left:760px;
	position: absolute;
}
/** Styling the navigational menus by adding a subtle background, and padding **/
ul.{#prefixSupport#}section_menu{
	background:#ededed;
	padding:20px 10px;
}
ul.{#prefixSupport#}section_menu li{
	margin-left:30px;
	list-style: square outside none !important;
}
ul.{#prefixSupport#}section_menu li a{
	color: #0986e3;
	list-style-type: circle;
	text-decoration:underline;
}
ul.{#prefixSupport#}section_menu li a:hover{
	color:#147FCF;
}

/** Style our faq questions **/
dl.{#prefixSupport#}faq{
	margin-bottom:30px;
}

/** Make the question itself stand out **/
dl.{#prefixSupport#}faq dt{
	font-weight:bold;
	color:#000000;
	padding:25px 0 5px 0;
	display:block;
}

/** Add a divisor below the question answer **/
dl.{#prefixSupport#}faq dd{
	padding-bottom:25px;
	border-bottom:1px solid #cccccc;
	display:block;
	color: #666666;
}

.{#prefixSupport#}title{
	font-family: 'Segoe UI', Arial, Sans-Serif;
	font-weight: 700;
	font-size: 2em;
	line-height: 50px;
	-webkit-font-feature-settings: "kern" 1;
	-moz-font-feature-settings: "kern" 1;
	font-feature-settings: "kern" 1;
	margin-bottom: 15px;
}
.{#prefixSupport#}note{
	color: #636468;
	font-family: 'Segoe UI', Arial, Sans-Serif;
	font-size: 14px;
	font-weight: 700;
	line-height: 1.6;
}
h4.{#prefixSupport#}note{
	margin: 10px 0;
	padding-right: 20px;
}
.{#prefixSupport#}border{
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	-ms-border-radius: 4px;
	-o-border-radius: 4px;
	border-radius: 4px;
	-webkit-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	-moz-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	-ms-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	-o-box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
	box-shadow: 0 1px 5px rgba(0, 0, 0, 0.2);
}
#{#prefixSupport#}block{
	background: #fff;
	float: left;
	margin: 10px 10px 20px 10px;
	font-family: 'WOL_SL','Segoe UI Semilight','Segoe UI',Tahoma,Helvetica,sans-serif;
	font-size: 14px;
	max-width: 600px;
	padding:40px;
}
#{#prefixSupport#}block .fullImage{
	width: 100%;
	margin: 20px 0;
}
#{#prefixSupport#}block .smallImage{
	width: 100px;
	margin: 20px 0;
}
#{#prefixSupport#}block a{
	color: #2793e6;
}
#{#prefixSupport#}block a:hover{
	text-decoration:underline;
}
#{#prefixSupport#}fullBlock{
	background: #fff;
	margin:50px 10px 10px 10px;
	padding-bottom: 40px;
	text-align: center;
	font-family: 'Whitney SSm A', 'Whitney SSm B', "ff-meta-web-pro", sans-serif;
	font-size:14px;
}
.{#prefixSupport#}subtitle{
	margin: 20px 0 10px;
	color: #336699 !important;
}
.{#prefixSupport#}link_title{
	margin-top: 10px !important;
	border-bottom: 1px solid #cccccc !important;
	line-height: 18px;
	padding-bottom: 2px;
	border-bottom: 2px solid #dbdbdb;
	margin: 40px 20px 10px 20px;
	font-size: 16px;
	height: 18px;
	font-weight: bold;
}
.{#prefixSupport#}link_title h2 a, .{#prefixSupport#}link_title a{
	color: #444;
	text-decoration: none;
	padding-bottom: 2px;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-weight: bold;
	line-height: 1.6;
	font-size: 14px;
}
.{#prefixSupport#}link_section{
	margin-left: 30px;
	display: inline-block;
}
.{#prefixSupport#}link_section a{
	color: #0986e3;
	font-size: 12px;
	text-decoration: underline;
}
.{#prefixSupport#}link_section a:hover{
	color: {$lightColor};
}
.{#prefixSupport#}badge_row{
	border-top:1px solid #e5e5e5;
	margin-top:4px;
	padding:10px 15px;
}
.{#prefixSupport#}badge_row:hover div{
	text-decoration:underline;
}
.{#prefixSupport#}badge_collumn{
	display:inline-block;
	padding:0 10px;
}
/* FOR USE IN PERMISSIONS */
.{#prefixSupport#}privilege_row{
	border-top:1px solid #e5e5e5;
	margin-top:4px;
	padding:10px 15px;
}
.{#prefixSupport#}privilege_row:hover{
	opacity:1 !important;
}
.{#prefixSupport#}privilege_row:hover div{
	text-decoration:underline;
}
.{#prefixSupport#}privilege_points{
	width:50px;
	display:inline-block;
	padding:0 10px;
}
.{#prefixSupport#}privilege_name{
	width:200px;
	display:inline-block;
	padding:0 10px;
}
.{#prefixSupport#}privilege_description{
	width:400px;
	display:inline-block;
	padding:0 10px;
}
.{#prefixSupport#}privilege_progress{
	width:100px;
	display:inline-block;
	padding:0 10px;
}
.{#prefixSupport#}progress_bar{
	width: 76px;
	margin: 0 auto;
	height: 12px;
	position: relative;
	background-color: #E2E2E2;
	border: 1px solid #E2E2E2;
}
.{#prefixSupport#}progress{
	background-repeat: no-repeat;
	background-position: left top;
	background-color: #8fc77e;
	height:12px;
	position: relative;
	text-align: left;
	background-size: 10px 10px;
}
/* FOR USE IN GROUPS */
.{#prefixSupport#}group_row{
	border-top:1px solid #e5e5e5;
	margin-top:4px;
	padding:10px 15px;
}
.{#prefixSupport#}group_row:hover{
	opacity:1 !important;
}
.{#prefixSupport#}group_row:hover div{
	text-decoration:underline;
}
.{#prefixSupport#}group_points{
	width:50px;
	display:inline-block;
	padding:0 10px;
}
.{#prefixSupport#}group_name{
	width:100px;
	display:inline-block;
	padding:0 10px;
}
.{#prefixSupport#}group_description{
	width:700px;
	display:inline-block;
	padding:0 10px;
}
/* FOR USE IN SITEMAP */
.{#prefixSupport#}sitemap {
	margin: 0 0 40px 0;
	float: left;
	width: 100%;
	padding-top:40px;
}
.{#prefixSupport#}sitemap a:link:before,
.{#prefixSupport#}sitemap a:visited:before {
	content: " "attr(href)" ";
	display: block;
	text-transform: uppercase;
	font-size: 10px;
	margin-bottom: 5px;
	word-wrap: break-word;
}
#{#prefixSupport#}primaryNav {
	margin: 0;
	float: left;
	width: 100%;
}
#{#prefixSupport#}primaryNav #{#prefixSupport#}home {
	display: block;
	float: none;
	background: #ffffff url('http://techberry.org/help/sitemap/images/L1-left.png') center bottom no-repeat;
	position: relative;
	z-index: 2;
	padding: 0 0 30px 0;
}
#{#prefixSupport#}primaryNav li {
	float: left;
	background: url('http://techberry.org/help/sitemap/images/L1-center.png') center top no-repeat;
	padding: 30px 0;
	width:300px;
	margin-top: -30px;
}
#{#prefixSupport#}primaryNav li a {
	margin: 0 20px 0 0;
	padding: 10px 0;
	display: block;
	font-size: 14px;
	font-weight: bold;
	text-align: center;
	color: black;
	background: #fff;
	border: 2px solid #ccc;
}
#{#prefixSupport#}primaryNav li a:hover {
	-moz-box-shadow: 2px 2px 4px rgba(0,0,0,0.5);
	-webkit-box-shadow: 2px 2px 4px rgba(0,0,0,0.5);
	box-shadow: 2px 2px 4px rgba(0,0,0,0.5);
	filter: filter: progid:DXImageTransform.Microsoft.Shadow(direction=135,strength=3, color=#808080);
}
#{#prefixSupport#}primaryNav li:last-child {
	background: url('http://techberry.org/help/sitemap/images/L1-right.png') center top no-repeat;
}
#{#prefixSupport#}primaryNav li a:link:before,
#{#prefixSupport#}primaryNav li a:visited:before {
	color: #78a9c0;
}

/* --------	Second Level --------- */

#{#prefixSupport#}primaryNav li li {
	width: 100%;
	clear: left;
	margin-top: 0;
	padding: 10px 0 0 0;
	background: url('http://techberry.org/help/sitemap/images/vertical-line.png') center bottom repeat-y;
}
#{#prefixSupport#}primaryNav li li a {
	background: #fff;
	border: 2px solid #ccc;
}
#{#prefixSupport#}primaryNav li li a:hover {
	-moz-box-shadow: 2px 2px 4px rgba(0,0,0,0.5);
	-webkit-box-shadow: 2px 2px 4px rgba(0,0,0,0.5);
	box-shadow: 2px 2px 4px rgba(0,0,0,0.5);
	filter: filter: progid:DXImageTransform.Microsoft.Shadow(direction=135,strength=3, color=#808080);
}
#{#prefixSupport#}primaryNav li li:first-child {
	padding-top: 30px;
}
#{#prefixSupport#}primaryNav li li:last-child {
	background: url('http://techberry.org/help/sitemap/images/vertical-line.png') center bottom repeat-y;
}
#{#prefixSupport#}primaryNav li li a:link:before,
#{#prefixSupport#}primaryNav li li a:visited:before {
	color: #{#prefixSupport#}8faf5c;
}

/* --------	Third Level --------- */

#{#prefixSupport#}primaryNav li li ul {
	margin: 10px 0 0 0;
	width: 100%;
	float: right;
	padding: 9px 0 10px 0;
	background: #ffffff url('http://techberry.org/help/sitemap/images/L3-ul-top.png') center top no-repeat;
}
#{#prefixSupport#}primaryNav li li li {
	background: url('http://techberry.org/help/sitemap/images/L3-center.png') left center no-repeat;
	padding: 5px 0;
}
#{#prefixSupport#}primaryNav li li li a {
	background-color: #}fff7aa;
	border-color: #e3ca4b;
	font-size: 12px;
	padding: 5px 0;
	width: 80%;
	float: right;
}
#{#prefixSupport#}primaryNav li li li a:hover {
	background-color: #fffce5;
	border-color: #d1b62c;
}
#{#prefixSupport#}primaryNav li li li:first-child {
	padding: 15px 0 5px 0;
	background: url('http://techberry.org/help/sitemap/images/L3-li-top.png') left center no-repeat;
}
#{#prefixSupport#}primaryNav li li li:last-child {
	background: url('http://techberry.org/help/sitemap/images/L3-bottom.png') left center no-repeat;
}
#{#prefixSupport#}primaryNav li li li a:link:before,
#{#prefixSupport#}primaryNav li li li a:visited:before {
	color: #{#prefixSupport#}ccae14;
	font-size: 9px;
}


/* ------------------------------------------------------------
	Utility Navigation
------------------------------------------------------------ */

#{#prefixSupport#}utilityNav {
	float: right;
	max-width: 50%;
	margin-right: 10px;
}
#{#prefixSupport#}utilityNav li {
	float: left;
	margin-bottom: 10px;
}
#{#prefixSupport#}utilityNav li a {
	margin: 0 10px 0 0;
	padding: 5px 10px;
	display: block;	
	border: 2px solid #e3ca4b;
	font-size: 12px;
	font-weight: bold;
	text-align: center;
	color: black;
	background: #fff7aa url('http://techberry.org/help/sitemap/images/white-highlight.png') top left repeat-x;
	-webkit-box-shadow: rgba(0,0,0,0.5) 2px 2px 2px; 
	-moz-box-shadow: rgba(0,0,0,0.5) 2px 2px 2px; /* FF 3.5+ */	
}
#{#prefixSupport#}utilityNav li a:hover {
	background-color: #fffce5;
	border-color: #d1b62c;
}
#{#prefixSupport#}utilityNav li a:link:before,
#{#prefixSupport#}utilityNav li a:visited:before {
	color: #{#prefixSupport#}ccae14;
	font-size: 9px;
	margin-bottom: 3px;
}
.{#prefixSupport#}earnt{
	width: 20px;
	height: 20px;
	vertical-align: middle;
	display: inline-block;
	background-image: url('http://techberry.org/images/tick.png');
	background-repeat: no-repeat;
	overflow: hidden;
}
.{#prefixSupport#}list{
	margin-left:20px;
	list-style-type:square;
}
.{#prefixSupport#}list li{
	margin-bottom:5px;
	font-weight:bold;
}
/* URL PAGE */
.{#prefixSupport#}imageWrap{
	margin: 20px 0;
	padding: 0 280px;
	display: block;
	overflow: hidden;
}
.{#prefixSupport#}imageSide{
	float:left;
	width:100px;
	margin: 0 5px;
	height:100px;
}
.{#prefixSupport#}urlActions{
	overflow: hidden;
	margin: 0 auto;
	width: 200px;
}
{/strip}