{strip}
.{#prefixProducts#}light{
    line-height: 18px;
    padding-bottom: 2px;
    border-bottom: 2px solid #dbdbdb;
    margin: 40px 20px 10px 20px;
    font-size: 16px;
    height: 18px;
    font-weight: bold;
}
.{#prefixProducts#}light h2{
    position: relative;
    float: left;
}
.{#prefixProducts#}light a{
    color: #444;
    text-decoration: none;
    padding-bottom: 2px;
    font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
    font-weight: bold;
    line-height: 1.6;
    font-size: 14px;
}
.{#prefixProducts#}light a:hover{
    border-bottom: 2px solid black;    
}
.{#prefixProducts#}{#subPrefixWrap#}{
    font-family: arial,sans-serif;
    margin-left: 20px;
}
.{#prefixProducts#}{#subPrefixWrap#} p{
    color: #444;
    font-size: 12px;
    font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
    font-weight: bold;
    line-height: 1.6;
}
.{#prefixProducts#}{#subPrefixWrap#} img{
    float: left;
    padding: 0 1em 0 0;
    margin: 5px 0 0;
}
.{#prefixProducts#}unsupported{
	opacity:0.3;
   transition: opacity .25s ease-in-out;
   -moz-transition: opacity .25s ease-in-out;
   -webkit-transition: opacity .25s ease-in-out;
}
.{#prefixProducts#}unsupported:hover{
	opacity:1;
}
.{#prefixProducts#}sub-title{
	color: #444;
	text-decoration: none;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-weight: bold;
	font-size: 18px;
	margin-left: 20px;
	margin-top: 10px;
}
.{#prefixProducts#}sub-text{
	margin-left: 20px;
	font-size: 12px;
	margin-top: 15px;
	color: #757474;
	font-weight: bold;
}
{/strip}