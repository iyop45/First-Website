{strip}
.{#prefixUpload#}title{
	padding-bottom: 10px;
	font-family: "Droid Sans", sans-serif;
	font-weight: bold;
	font-size: 25px;
}
p.{#prefixUpload#}note{
	font-size: 13px;
	line-height: 1.2;
	margin-bottom: 6px;
	font-weight:bolder;
}
p.{#prefixUpload#}note a{
	color:#2793e6;
}
p.{#prefixUpload#}note a:hover{
	text-decoration:underline;
}
#{#prefixUpload#}status{
	font-size: 90%;
	font-style: italic;
	display:none;
	margin-left: 10px;
	margin-bottom: 10px;
}
#{#prefixUpload#}drag{
	text-align: center;
	font-weight: bolder;
	padding: 1em 0;
	margin: 1em 0;
	color: #555;
	border: 2px dashed #888;
	border-radius: 7px;
	cursor: default;
	min-height:300px;
}
#{#prefixUpload#}drag.hover{
	border: 2px dashed #000;
}
#{#prefixUpload#}drag img{
	margin-top:40px;
}
.{#prefixUpload#}buttons{
	float:right;
}
.{#prefixUpload#}result{
	margin-bottom: 8px;
	margin-left: 8px;
}
.{#prefixUpload#}result a{
	color:#2793e6;
}
.{#prefixUpload#}result a:hover{
	text-decoration:underline;
}
{/strip}