{strip}
#{#prefixIndex#}section{
	width: 100%;
	height: 280px;
	position: relative;
	font-family: {#sff_arial#};
	margin-bottom: 15px;
}
.{#prefixIndex#}form{
	width: 100%;
}
.{#prefixIndex#}box{
	box-shadow: 0px 1px 3px rgba(0,0,0,.3);
	float: left;
	width: 310px;
	margin: 0 5px;
}
.{#prefixIndex#}box .xt{
	background: #ebebf4;
	color: #000;
	text-shadow: 0 -1px 0 rgba(0,0,0,.15);
}
.{#prefixIndex#}box .b_t{
	color: #fff;
	text-align: center;
	line-height: 40px;
	font-size: 18px;
}
.{#prefixIndex#}box .p1{
	background-color: #444449;
	border-bottom: 2px solid {$darkColor};
}
.{#prefixIndex#}box .p2{
	background-color: #444449;
	border-bottom: 2px solid {$darkColor};
}
.{#prefixIndex#}box .p3{
	background-color: #444449;
	border-bottom: 2px solid {$darkColor};
}
.{#prefixIndex#}box .xt p{
	margin: 0px;
	padding: 10px 0px;
	text-indent: 15px;
	border-bottom: 1px solid #ccc;
	font-size: 12px;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	font-weight: bold;
	text-shadow: none;
}
#{#prefixIndex#}player{
	margin-left:35px;
	display: inline-block;
}
.{#prefixIndex#}collumn{
	position: relative;
	width: 320px;
	height: 100%;
	border: 1px solid #dbdbeb;
	display: inline-block;
}
.{#prefixIndex#}register{
	margin-left:5px;
	width:390px !important;
	padding:0 10px;
	float:left;
	height:auto !important;
}
#{#prefixIndex#}msg{
	margin-bottom:10px;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-weight:bold;
	line-height:1.6;
	font-size:12px;
}
.{#prefixIndex#}ind_reg{
	background-color: #E5E8E8;
	border: 1px solid #dbdbeb;
}
.{#prefixIndex#}ind_reg form p, .{#prefixIndex#}ind_reg form input{
	margin-left: 0px !important;
}
.{#prefixIndex#}ind_reg form div input{
	border-radius: 0px !important;
	margin-bottom: 8px !important;
	font-size: 18px !important;
	border: 1px solid #ccc;
	line-height:44px !important;
	height:44px;
	padding:0px;
	padding-left: 10px;
	width: 310px !important;
}
.{#prefixIndex#}ind_reg form div input:hover{
	border: 1px solid #b9b9b9;
	border-top-color: #a0a0a0;
	-moz-box-shadow: inset 0 1px 2px rgba(0,0,0,.1);
	-webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,.1);
	box-shadow: inset 0 1px 2px rgba(0,0,0,.1);
	outline: none;
}
#{#prefixIndex#}{#subPrefixHeader#}{
	font-family: {#sff_arial#};
	font-size: 22pt;
	font-weight: normal;
	color: #000;
	padding: 6px 0px 7px 0px;
	margin: 0px;
}
#{#prefixIndex#}{#subPrefixWrap#}_snippet{
	max-width: 500px;
	border: 1px solid {$darkColor};
	margin-bottom: 15px;
	border-radius: 2px;
	margin-bottom: 20px;
}
#{#prefixIndex#}snippet{
	overflow: hidden;
	word-wrap: no-wrap;
	border-radius: 2px;
	padding: 10px 0;
	background: #ebebf4;
	border: 1px solid #dbdbeb;
	background-color: rgb(234, 234, 234);
	color: #000;
	border-color: 1px solid #dbdbeb;
}
#{#prefixIndex#}snippet p{
	margin: 0 20px;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-weight: bold;
	line-height: 1.6;
	font-size: 12px;
}
{/strip}