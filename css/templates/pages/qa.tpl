{strip}
.{#prefixQA#}list{
	width: 100%;
	border-bottom: 1px solid #F0F0F0;
	box-shadow: 0 1px 0 #fff;
	-webkit-box-shadow: 0 1px 0 #fff;
	-moz-box-shadow: 0 1px 0 #fff;
	padding: 15px 0;
	overflow:hidden;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-size: 14px;
	color: #444;
	line-height: 1.4;
	font-weight:bolder;
}
.{#prefixQA#}reply_content{
	margin-left: 20px;
	padding-top: 4px;
	width: 800px;
	float: left;
}
.{#prefixQA#}reply_content .light h2{
	margin-bottom:10px;
}
.{#prefixQA#}title{
	margin:.8em 1em .8em 1em;
	float:left;
	font-weight:700;
}
.{#prefixQA#}directory_link{
	margin:.8em 1em .8em 1em;
	float:left;
	font-weight:700;
}
.{#prefixQA#}directory_link a{
	color:#444;
	text-decoration:none;
}
.{#prefixQA#}directory_link a:hover{
	text-decoration:underline;
}
.{#prefixQA#}list_info{
	width: 260px;
	margin-left: 20px;
	padding-top: 4px;
	float: left;
	overflow:hidden;
	text-overflow:ellipsis;
}
.{#prefixQA#}list_info .{#prefixQA#}list_description{
	font-size: 13px;
	line-height: 1.2;
	margin-bottom: 6px;
}
.{#prefixQA#}comment_box{
	box-shadow: 0 1px 0 #fff;
	-webkit-box-shadow: 0 1px 0 #fff;
	-moz-box-shadow: 0 1px 0 #fff;
	overflow: hidden;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-size: 14px;
	margin:0 92px;
	color: #444;
	line-height: 1.4;
	font-weight: bolder;
	border: 1px solid #E9E9E9;
	border-top: none;
}
.{#prefixQA#}side_list{
	float:left;
}
.{#prefixQA#}side_list li{
	overflow:hidden;
}
.{#prefixQA#}accepted{
	width: 50px;
	padding:2px 18px;
	height: 50px;
}
.{#prefixQA#}votes{
	width: 50px;
	padding:2px 18px;
}
.{#prefixQA#}vote_number{
	text-align:center;
	padding-top: 2px;
	color: #555;
	font-size:16px;
}
.{#prefixQA#}vote_icon{
	text-align:center;
	cursor:pointer;
}
.{#prefixQA#}vote_icon:hover{
	opacity:0.4;
}
.{#prefixQA#}comment_box .{#prefixQA#}comment{
	padding:5px;
	background: #F5F5F5;
}
.{#prefixQA#}comment_box .{#prefixQA#}comment .{#prefixQA#}comment_votes{
	font-size: 14px;
	color: #666666;
	font-weight: bold;
	padding-right: 2px;
}
.{#prefixQA#}comment_box .{#prefixQA#}comment .{#prefixQA#}comment_upvote{
	background:url('http://techberry.org/answers/images/upvote.png') no-repeat center center fixed;
	-webkit-background-size: cover; /* For WebKit*/
	-moz-background-size: cover;    /* Mozilla*/
	-o-background-size: cover;      /* Opera*/
	background-size: cover;         /* Generic*/
	width:15px;
	height:15px;
}
.{#prefixQA#}list_info a h2{
	line-height: 1.2;
	margin:0 0 4px 0;
}
.{#prefixQA#}list_info a{
	color: #366FB3;
	text-decoration: none;
	cursor: pointer;
}
.{#prefixQA#}list_info a:hover{
	color: #4CA3D7;
}
.{#prefixQA#}list_stats{
	float: right;
	width: 600px;
}
.{#prefixQA#}list_stats .{#prefixQA#}list_box{
	width: 80px;
	height: 63px;
	text-align: center;
	color: #999;
	font-size: 12px;
	float: left;
	margin-left: 5px;
}
.{#prefixQA#}list_box.accepted{
	background: #9c9;
	color: #eee;
}
.{#prefixQA#}list_box.accepted span{
	color: #eee !important;
}
.{#prefixQA#}list_stats .{#prefixQA#}list_box_action{
	height: 63px;
	text-align: center;
	color: #999;
	float: left;
	margin-left: 5px;
}
.{#prefixQA#}list_stats .{#prefixQA#}list_box_meter{
	height: 63px;
	text-align: center;
	color: #999;
	float: left;
	margin-left: 5px;
	width:200px;
}
.{#prefixQA#}list_stats .{#prefixQA#}list_box .{#prefixQA#}list_value{
	color: #555;
	display: inline-block;
	width: 100%;
	font-size: 20px;
	font-family: Arial,Helvetica,sans-serif;
	line-height: 1.6;
}
.{#prefixQA#}list_bubble{
	color: #fff;
	text-align: center;
	float: left;
	line-height: 1.3;
	border-width: 0;
	width: 60px;
	height: 60px;
	position: relative;
	padding: 8px;
	border: 1px solid #C9C9C9;
}
.{#prefixQA#}list_bubble:hover{
	box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
	-webkit-box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
	-moz-box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
}
.{#prefixQA#}list_bubble .{#prefixQA#}list_icon{
	width: 42px;
	height: 42px;
	margin-top: 10px;
}
.{#prefixQA#}reply{
	margin: 0 auto;
	text-align: center;
}
{/strip}