{strip}
.{#prefixTutorials#}list{
	width: 100%;
	border-bottom: 1px solid #F0F0F0;
	box-shadow: 0 1px 0 #fff;
	-webkit-box-shadow: 0 1px 0 #fff;
	-moz-box-shadow: 0 1px 0 #fff;
	padding: 15px 0;
	overflow:hidden;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-size: 14px;
	color: #444;
	line-height: 1.4;
	font-weight:bolder;
}
.{#prefixTutorials#}title_block{
	overflow:hidden;
}
.{#prefixTutorials#}title{
	margin:.8em 1em .8em 1em;
	float:left;
	font-weight:700;
}
.{#prefixTutorials#}content{
	overflow:hidden;
}
.{#prefixTutorials#}directory_link{
	margin:.8em 1em .8em 1em;
	float:left;
	font-weight:700;
}
.{#prefixTutorials#}directory_link a{
	color:#444;
	text-decoration:none;
}
.{#prefixTutorials#}directory_link a:hover{
	text-decoration:underline;
}
.{#prefixTutorials#}list_info{
	width: 260px;
	margin-left: 20px;
	padding-top: 4px;
	float: left;
	overflow:hidden;
	text-overflow:ellipsis;
}
.{#prefixTutorials#}list_info .{#prefixTutorials#}list_description{
	font-size: 13px;
	line-height: 1.2;
	margin-bottom: 6px;
}

.{#prefixTutorials#}list_info a h2{
	line-height: 1.2;
	margin:0 0 4px 0;
}
.{#prefixTutorials#}list_info a{
	color: #366FB3;
	text-decoration: none;
	cursor: pointer;
}
.{#prefixTutorials#}list_info a:hover{
	color: #4CA3D7;
}
.{#prefixTutorials#}list_stats{
	float: right;
	width: 600px;
}
.{#prefixTutorials#}list_stats .{#prefixTutorials#}list_box{
	width: 80px;
	height: 63px;
	text-align: center;
	color: #999;
	font-size: 12px;
	float: left;
	margin-left: 5px;
}
.{#prefixTutorials#}list_stats .{#prefixTutorials#}list_box_action{
	height: 63px;
	text-align: center;
	color: #999;
	float: left;
	margin-left: 5px;
}
.{#prefixTutorials#}list_stats .{#prefixTutorials#}list_box_meter{
	height: 63px;
	text-align: center;
	color: #999;
	float: left;
	margin-left: 5px;
	width:200px;
}
.{#prefixTutorials#}list_stats .{#prefixTutorials#}list_box .{#prefixTutorials#}list_value{
	color: #555;
	display: inline-block;
	width: 100%;
	font-size: 20px;
	font-family: Arial,Helvetica,sans-serif;
	line-height: 1.6;
}
.{#prefixTutorials#}list_bubble{
	color: #fff;
	text-align: center;
	float: left;
	line-height: 1.3;
	border-width: 0;
	width: 60px;
	height: 60px;
	position: relative;
	padding: 8px;
	border: 1px solid #C9C9C9;
}
.{#prefixTutorials#}list_bubble:hover{
	box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
	-webkit-box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
	-moz-box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
}
.{#prefixTutorials#}list_bubble .{#prefixTutorials#}list_icon{
	width: 42px;
	height: 42px;
	margin-top: 10px;
}
.{#prefixTutorials#}center_message{
	margin-left: 20px;
	padding-top: 4px;
	overflow: hidden;
	width: 800px;
	text-align: center;
	margin: 0 auto;
	margin-bottom: 6px;
	font-weight: 700;
	color: #444;
}
.{#prefixTutorials#}page_message{
	margin-top: 20px;
	padding-top: 10px;
	border-top: 1px solid #F0F0F0;
}
.{#prefixTutorials#}comment_box{
	background-color: #fff;
	border: 1px solid #e5e5e5;
	border-bottom: 1px solid #eee;
	margin-top:20px;
}
.{#prefixTutorials#}comment_holder{
	padding: 18px 22px 10px 22px;
}
textarea.{#prefixTutorials#}comment_textarea{
	background-color: #fff;
	border: 1px solid #e5e5e5;
	box-sizing: border-box;
	margin: 2px 0px;
	color: #222;
	display: inline-block;
	font-family: arial;
	max-width: 100%;
	font-size: 13px;
	padding: 5px 2px 2px 5px;
	vertical-align: top;
	width: 100%;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-webkit-border-radius: 1px;
	-webkit-appearance: none;
}
.{#prefixTutorials#}comment_buttons{
	background-color: #f5f5f5;
	padding: 2px 5px 8px 2px;
	overflow:hidden;
}
.{#prefixTutorials#}comment_buttons input{
	margin: 8px 0 0 18px;
	float:left;
}
{/strip}