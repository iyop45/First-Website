{strip}
.{#prefixForum#}list{
	width: 100%;
	border-bottom: 1px solid #F0F0F0;
	box-shadow: 0 1px 0 #fff;
	-webkit-box-shadow: 0 1px 0 #fff;
	-moz-box-shadow: 0 1px 0 #fff;
	padding: 15px 0;
	overflow:hidden;
	font-family: 'Helvetica Neue',Helvetica,Arial,Sans-serif;
	font-size: 14px;
	color: #444;
	line-height: 1.4;
	font-weight:bolder;
}
.{#prefixForum#}reply_content{
	margin-left: 20px;
	padding-top: 4px;
	width: 800px;
	float: left;
}
h1.{#prefixForum#}subtitle{
	font-size: 20px;
}
.{#prefixForum#}reply_content .light h2{
	margin-bottom:10px;
}
.{#prefixForum#}title{
	margin:.8em 1em .8em 1em;
	float:left;
	font-weight:700;
}
.{#prefixForum#}directory_link{
	margin:.8em 1em .8em 1em;
	float:left;
	font-weight:700;
}
.{#prefixForum#}directory_link a{
	color:#444;
	text-decoration:none;
}
.{#prefixForum#}directory_link a:hover{
	text-decoration:underline;
}
.{#prefixForum#}list_info{
	width: 260px;
	margin-left: 20px;
	padding-top: 4px;
	float: left;
	overflow:hidden;
	text-overflow:ellipsis;
}
.{#prefixForum#}list_info .{#prefixForum#}list_description{
	font-size: 13px;
	line-height: 1.2;
	margin-bottom: 6px;
	width: 260px;
}
.{#prefixForum#}list_info a h2{
	line-height: 1.2;
	margin:0 0 4px 0;
}
.{#prefixForum#}list_info a{
	color: #366FB3;
	text-decoration: none;
	cursor: pointer;
}
.{#prefixForum#}list_info a:hover{
	color: #4CA3D7;
}
.{#prefixForum#}list_stats{
	float: right;
	width: 600px;
}
.{#prefixForum#}list_stats .{#prefixForum#}list_box{
	width: 80px;
	height: 63px;
	text-align: center;
	color: #999;
	font-size: 12px;
	float: left;
	margin-left: 5px;
}
.{#prefixForum#}list_stats .{#prefixForum#}list_box_action{
	height: 63px;
	text-align: center;
	color: #999;
	float: left;
	margin-left: 5px;
}
.{#prefixForum#}list_stats .{#prefixForum#}list_box_meter{
	height: 63px;
	text-align: center;
	color: #999;
	float: left;
	margin-left: 5px;
	width:200px;
}
.{#prefixForum#}list_stats .{#prefixForum#}list_box .{#prefixForum#}list_value{
	color: #555;
	display: inline-block;
	width: 100%;
	font-size: 20px;
	font-family: Arial,Helvetica,sans-serif;
	line-height: 1.6;
}
.{#prefixForum#}list_bubble{
	color: #fff;
	text-align: center;
	float: left;
	line-height: 1.3;
	border-width: 0;
	width: 60px;
	height: 60px;
	position: relative;
	margin-bottom:10px;
	padding: 8px;
	border: 1px solid #C9C9C9;
}
.{#prefixForum#}list_bubble:hover{
	box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
	-webkit-box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
	-moz-box-shadow: 2px 2px 4px rgba(0,0,0,0.1);
}
.{#prefixForum#}list_bubble .{#prefixForum#}list_icon{
	width: 42px;
	height: 42px;
	margin-top: 10px;
}
.{#prefixForum#}center_message{
	margin-left: 20px;
	padding-top: 4px;
	overflow: hidden;
	width: 800px;
	text-align: center;
	line-height: 20px;
	margin: 0 auto;
	margin-bottom: 6px;
	font-weight: 700;
	color: #444;
}
.{#prefixForum#}center_message a:hover{
	text-decoration:underline;
}
.{#prefixForum#}side_list{
	float:left;
}
.{#prefixForum#}side_list .{#prefixForum#}side_list_actions{
	overflow: hidden;
	width: 78px;
	text-align:center;
}
.{#prefixForum#}info_message{
	background-color: #F8E7BD;
	color: #444444;
	padding: 10px;
	margin-bottom: 10px;
}
.{#prefixForum#}edit_content, .{#prefixForum#}create_content{
	margin-left: 20px;
	padding-top: 4px;
	width: 800px;
	float: left;
}
.{#prefixForum#}edit_content .input_text, .{#prefixForum#}create_content .input_text{
	width:774px;
	border: 3px solid #dbdbdb;
	margin:0;
	height:20px;
	padding:10px;
	margin-bottom:10px;
}
.{#prefixForum#}edit_content select{
	width:800px;
	border: 3px solid #dbdbdb;
	margin:0;
	padding:10px;
	margin-bottom:10px;
}
.{#prefixForum#}edit_content .textarea, .{#prefixForum#}create_content .textarea{
	width:774px;
	border: 3px solid #dbdbdb;
	margin:0;
	padding:10px;
	min-height:300px;
}
.{#prefixForum#}votes{
	width: 50px;
	padding:2px 18px;
}
.{#prefixForum#}vote_number{
	text-align:center;
	padding-top: 2px;
	color: #555;
	font-size:16px;
}
.{#prefixForum#}vote_icon{
	text-align:center;
}
.{#prefixForum#}edit_actions{
	margin:0 10px 0 10px;
}
.{#prefixForum#}edit_button{
	margin:5px 0;
	text-align:center;
}
.{#prefixForum#}loadMore{
	border: 1px solid;
	border-color: #d3d3d3;
	width: 200px;
	outline: 0;
	border-width: 1px 1px 1px 1px;
	box-shadow: inset 1px 1px 0 rgba(0,0,0,.1),inset 0 -1px 0 rgba(0,0,0,.07);
	font-weight: bold;
	background-image: -webkit-linear-gradient(top,#fcfcfc 0,#f8f8f8 100%);
	background-color: rgba(0,0,0,.2);
	background-clip: padding-box;
	margin: 0 auto;
	-webkit-border-radius: 3px;
	padding: 20px;
	min-width: 400px;
	cursor:pointer;
	border-radius: 3px;
	text-align:center;
	font-size: 12px;
	color: #333;
	margin-top:10px;
	line-height: 1.4;
}
.{#prefixForum#}loadMore:hover{
	background-color:#f0f0f0;
	background-image:-webkit-linear-gradient(bottom,#f0f0f0 0,#f8f8f8 100%);
	background-image:linear-gradient(bottom,#f0f0f0 0,#f8f8f8 100%);
	border-color:#c6c6c6;
	text-decoration:none;
}
.{#prefixForum#}newReplies{
	margin: 0px 10px 0px 0px;
	padding: 8px;
	color:#ccc;
	text-align:center;
}
.{#prefixForum#}side_tip{
	background-color: #fff7e3;
	color: #444;
	border-radius: 5px;
	margin-top:4px;
	padding: 8px;
	float: right;
	width: 130px;
}
.{#prefixForum#}side_tip h4{
	font-size: 15px;
	padding-bottom: 8px;
	color: #222;
	font: 14px/18px Lucida Grande,Lucida Sans Unicode,Tahoma,sans-serif;
}
.{#prefixForum#}side_tip p{
	margin-bottom:10px;
}
{/strip}